{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      {
        "include": "src/generated/client/*.node",
        "outDir": "dist/generated/client",
        "watchAssets": false
      }
    ],
    "watchAssets": false
  }
}
